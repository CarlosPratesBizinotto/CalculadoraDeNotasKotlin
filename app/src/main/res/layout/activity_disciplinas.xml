<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Disciplinas">

    <Button
        android:id="@+id/imunologia"
        android:layout_width="269dp"
        android:layout_height="72dp"
        android:layout_marginBottom="560dp"
        android:text="Imunologia"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/anatomia"
        android:layout_width="269dp"
        android:layout_height="72dp"
        android:layout_marginBottom="432dp"
        android:text="Anatomia"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imunologia"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/fisiologia"
        android:layout_width="269dp"
        android:layout_height="72dp"
        android:text="Fisiologia"
        app:layout_constraintBottom_toTopOf="@+id/farmacologia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/anatomia"
        app:layout_constraintVertical_bias="0.509"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/farmacologia"
        android:layout_width="269dp"
        android:layout_height="72dp"
        android:layout_marginBottom="20dp"
        android:text="Farmacologia"
        app:layout_constraintBottom_toTopOf="@+id/nutricao"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/nutricao"
        android:layout_width="269dp"
        android:layout_height="72dp"
        android:layout_marginBottom="184dp"
        android:text="Nutrição Animal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="271dp"
        android:layout_height="39dp"
        android:text="Selecione a Diciplina"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imunologia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.642"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.666"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>